createLighttpdConf(){
   local file="$VAR_CONFIG_DIR/lighttpd.conf"
   local setup_begin=""
   local setup_end=";"
   local setup_dblUScore="."
   echo -e "setup {" > "$file"
   configFromVarGroup setup >> "$file"
   echo -e "}\n" >> "$file"
   local modeGroup="mode_$VAR_OPERATION_MODE"
   eval "configFromVarGroup mode_$VAR_OPERATION_MODE" >> "$file"
}
